<header class="header">
<div>
  <a href="<%- url_for() %>"><%= config.title %></a>
</div> 
<nav>
  <ul class="nav justify-content-center">
    <% for (item in theme.menu) { %>
    <li class="nav-item">
      <a class="nav-link" href="<%- url_for(theme.menu[item]) %>"> <%= item %></a>
    </li>
    <% } %>
  </ul>
</nav>
</header>

